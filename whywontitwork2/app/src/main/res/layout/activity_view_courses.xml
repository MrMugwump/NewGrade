                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/quicksand_light"
                    android:text="@string/RoomNumber"
                    android:textColor="@color/secondaryTextColor"
                    app:layout_constraintStart_toStartOf="@+id/periodSevenTeacher"
                    app:layout_constraintTop_toBottomOf="@+id/periodSevenTeacher" />

                <TextView
                    android:id="@+id/periodEightNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/quicksand_light"
                    android:text="@string/periodEight"
                    android:textColor="@color/secondaryTextColor"
                    app:layout_constraintStart_toStartOf="@+id/periodEight"
                    app:layout_constraintTop_toBottomOf="@+id/periodEightGrade" />

                <TextView
                    android:id="@+id/periodEightGrade"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/quicksand_light"
                    android:text="@string/courseGrade"
                    android:textSize="24sp"
                    app:layout_constraintStart_toStartOf="@+id/guideline14"
                    app:layout_constraintTop_toTopOf="@+id/periodEight" />

                <TextView
                    android:id="@+id/periodEightName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/quicksand_light"
                    android:text="@string/courseName"
                    app:layout_constraintStart_toStartOf="@+id/periodEight"
                    app:layout_constraintTop_toBottomOf="@+id/periodEightNumber" />

                <TextView
                    android:id="@+id/periodEightTeacher"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/quicksand_light"
                    android:text="@string/teacherName"
                    android:textColor="@color/secondaryTextColor"
                    app:layout_constraintStart_toStartOf="@+id/periodEightName"
                    app:layout_constraintTop_toBottomOf="@+id/periodEightName" />

                <TextView
                    android:id="@+id/periodEightRoom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/quicksand_light"
                    android:text="@string/RoomNumber"
                    android:textColor="@color/secondaryTextColor"
                    app:layout_constraintStart_toStartOf="@+id/periodEightTeacher"
                    app:layout_constraintTop_toBottomOf="@+id/periodEightTeacher" />

                <ImageView
                    android:id="@+id/periodSevenLetter"
                    android:layout_width="70dp"
                    android:layout_height="43dp"
                    android:contentDescription="@string/letter"
                    app:layout_constraintBottom_toBottomOf="@+id/periodSevenGrade"
                    app:layout_constraintEnd_toStartOf="@+id/guideline13"
                    app:layout_constraintTop_toTopOf="@+id/periodSevenGrade"
                    app:srcCompat="@drawable/b" />

                <ImageView
                    android:id="@+id/periodEightLetter"
                    android:layout_width="70dp"
                    android:layout_height="43dp"
                    android:contentDescription="@string/letter"
                    app:layout_constraintBottom_toBottomOf="@+id/periodEightGrade"
                    app:layout_constraintEnd_toStartOf="@+id/guideline14"
                    app:layout_constraintTop_toTopOf="@+id/periodEightGrade"
                    app:srcCompat="@drawable/b" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.25" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideline14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.75" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/GPA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/quicksand_light"
        android:text="@string/gpa"
        android:textColor="@color/colorAccent"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50"/>

    <Switch
        android:id="@+id/gpaSwitch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/quicksand_light"
        android:text="@string/WeightedGPA"
        android:textColor="@color/colorSecondary"
        android:textOff="@string/UnweightedGPA"
        android:textOn="@string/WeightedGPA"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/GPA" />

    <Button
        android:id="@+id/popupMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/rounded_corner"
        android:onClick="ShowPopup"
        android:text="@string/popupButton"
        android:textColor="@color/colorPrimary"
        android:textSize="8sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="38dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:background="@drawable/rounded_corner"
        android:onClick="GoBack"
        android:text="@string/backButton"
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

</androidx.constraintlayout.widget.ConstraintLayout>